{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "pnpm install --no-frozen-lockfile && pnpm build",
  "outputDirectory": ".next",
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1"
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ]
}
